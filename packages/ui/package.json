    "react-resizable-panels": "^1.0.5",
    "recharts": "^2.10.0",
    "sonner": "^1.3.1",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "lucide-react": "^0.525.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
