{
  "name": "@hanzo/ui",
  "version": "1.0.0",
  "description": "Complete UI library with shadcn and Tailwind UI components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/lazy": {
      "import": "./dist/components/lazy/index.mjs",
      "require": "./dist/components/lazy/index.js",
      "types": "./dist/components/lazy/index.d.ts"
    },
    "./blocks": {
      "import": "./dist/blocks/index.mjs",
      "require": "./dist/blocks/index.js",
      "types": "./dist/blocks/index.d.ts"
    },
    "./ai": {
      "import": "./dist/ai/index.mjs",
      "require": "./dist/ai/index.js",
      "types": "./dist/ai/index.d.ts"
    },
    "./lib/utils": {
      "import": "./dist/lib/utils.mjs",
      "require": "./dist/lib/utils.js",
      "types": "./dist/lib/utils.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "optionalDependencies": {
    "@hookform/resolvers": "^3.3.4",
    "cmdk": "^0.2.0",
    "date-fns": "^3.0.0",
    "embla-carousel-react": "^8.0.0",
